{
  "name": "ap-cse-app",
  "version": "0.0.0",
  "description": "React frontend for AP Content Specification Explorer",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "test": "jest --coverage",
    "test-ci": "jest --coverage --runInBand",
    "dev": "webpack-serve",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "format": "prettier --single-quote --print-width 100 --write src/**/*.{ts,tsx,js,json}",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix -p ./tsconfig.json",
      "prettier --single-quote --print-width 100 --write",
      "git add"
    ],
    "src/**/*.{js,json}": [
      "prettier --single-quote --print-width 100 --write",
      "git add"
    ],
    "**/*.{md}": [
      "prettier --single-quote --print-width 100 --write",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./.storybook/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js)$": "babel-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "output": "reports/jest/results.xml"
        }
      ]
    ],
    "testMatch": [
      "**/*.spec.+(tsx|ts|js)"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.tsx",
      "!**/*.storybook.skip.tsx"
    ]
  },
  "dependencies": {
    "@react-pdf/renderer": "^1.0.0-alpha.17",
    "@react-pdf/styled-components": "^1.2.0",
    "@sbac/sbac-ui-kit": "github:osu-cass/sbac-global-ui-kit#develop",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-feather": "^1.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "styled-jsx": "^3.0.2",
    "typeface-pt-sans-caption": "0.0.54",
    "typeface-pt-serif": "0.0.54"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@commitlint/cli": "^7.1.1",
    "@commitlint/config-conventional": "^7.1.1",
    "@osu-cass/tslint-config": "^2.0.0",
    "@storybook/addon-a11y": "^4.0.0-alpha.20",
    "@storybook/addon-actions": "^4.0.0-alpha.20",
    "@storybook/addon-centered": "^3.4.10",
    "@storybook/addon-console": "^1.0.4",
    "@storybook/addon-storyshots": "^4.0.0-alpha.18",
    "@storybook/react": "^4.0.0-alpha.20",
    "@types/enzyme": "^3.1.14",
    "@types/faker": "^4.1.3",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-scroll": "^1.5.4",
    "@types/storybook__addon-centered": "^3.3.1",
    "@types/storybook__react": "^3.0.9",
    "@types/styled-jsx": "^2.2.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^4.0.0-alpha",
    "husky": "^1.0.0-rc.13",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-junit": "^5.1.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.2",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.4.2",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "webpack": "^4.17.1",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osu-cass/AP-CSE-APP.git"
  },
  "author": "",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/osu-cass/AP-CSE-APP/issues"
  },
  "homepage": "https://github.com/osu-cass/AP-CSE-APP#readme"
}
